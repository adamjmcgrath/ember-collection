<div class="mixed" style="position:relative;height:500px">
  {{#ember-collection items=model estimated-height=800 estimated-width=500 buffer=10 cell-layout=(mixed-grid-layout model) as |item index itemSizeChange|}}
    {{#mixed-item heightUpdated=itemSizeChange index=index item=item }}
      {{item.name}}
    {{/mixed-item}}
  {{/ember-collection}}
</div>
